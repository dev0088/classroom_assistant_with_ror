{
  "name": "classroom-desktop",
  "productName": "GitHub Classroom",
  "version": "0.4.0",
  "description": "Desktop helper for GitHub Classroom",
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron main.js",
    "test": "npm run lint && npm run test-unit",
    "lint": "node_modules/.bin/eslint --ext .js,.jsx .",
    "test-unit": "electron-mocha -t 60000 --require babel-core/register app/**/*-test.js",
    "build-mac": "electron-packager . 'GitHub Classroom' --icon=app/resources/icon.icns --platform=darwin --arch=x64 --out=dist --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/education/classroom-desktop/"
  },
  "keywords": [],
