    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "~3.7.0",
    "eslint-plugin-react": "~7.7.0",
    "eslint-plugin-standard": "~3.0.1",
    "mocha": "~5.0.4",
    "redux-devtools": "~3.4.1",
    "redux-devtools-dock-monitor": "~1.1.1",
    "redux-devtools-log-monitor": "~1.4.0",
    "sinon": "^4.4.8"
